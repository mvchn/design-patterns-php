{
  "name": "mvchn/design-patterns-php",
  "description": "Design patterns in PHP",
  "type": "project",
  "license": "MIT",
  "require": {
    "php": ">=7.4"
  },
  "require-dev": {
    "phpunit/phpunit": "10.*"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": [
      "php vendor/bin/phpunit tests"
    ]
  }
}
